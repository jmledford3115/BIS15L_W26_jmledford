library("tidyverse")
library("janitor")
library("palmerpenguins") #load the palmerpenguins package
options(scipen=999) #turn off scientific notation
penguins %>%
select(species, island, body_mass_g) %>%
arrange(body_mass_g)
penguins %>%
filter(island=="Biscoe" | island=="Dream")
penguins %>%
filter(island==c("Biscoe", "Dream"))
penguins %>%
filter(island %in% c("Biscoe", "Dream"))
penguins %>%
select(body_mass_g, flipper_length_mm)
penguins %>%
select(body_mass_g, flipper_length_mm) %>%
ggplot(mapping=aes(x=flipper_length_mm, y=body_mass_g)) +
geom_point()+
geom_smooth(method="lm", se=F)
glimpse(penguins)
summary(penguins)
penguins %>%
distinct()
glimpse(penguins)
penguins %>%
distinct(species)
penguins %>%
distinct(species, island)
penguins %>%
distinct(species, island. keep_all=T)
penguins %>%
distinct(species, island,. keep_all=T)
penguins %>%
distinct(species, island,.keep_all=T)
penguins %>%
mutate(body_mass_kg=body_mass_g/1000)
penguins %>%
mutate(body_mass_kg=body_mass_g/1000) %>%
select(species, body_mass_g, body_mass_kg) %>%
arrange(body_mass_kg)
penguins %>%
mutate(across(ends_with("mm"), ~./10))
penguins %>%
mutate(across(ends_with("mm"), ~./10)) %>%
select(species,
bill_length_cm=bill_length_mm,
bill_depth_cm=bill_depth_mm,
flipper_length_cm=flipper_length_mm)
mammals <- read_csv("data/mammal_lifehistories_v2.csv")
glimpse(mammals)
mammals %>%
clean_names()
mammals <- mammals %>%
clean_names()
glimpse(mammals)
mammals <- read_csv("data/mammal_lifehistories_v2.csv") %>% clean_names()
glimpse(mammals)
mammals %>%
mutate(across(c("order", "family"), tolower)) #specific columns
mammals %>%
mutate(across(everything(), tolower))
mammals %>%
mutate(across(where(is.character), tolower))
summary(mammals)
mammals %>%
select(genus, species, newborn) %>%
arrange(newborn)
mammals %>%
select(genus, species, newborn)
mammals %>%
select(genus, species, newborn) %>%
mutate(newborn_new=ifelse(newborn == -999.00, NA, newborn))
mammals %>%
mutate(across(c(mass, wean_mass, gestation, max_life, newborn, weaning, litter_size, afr, litters_year),
~ifelse(.==-999, NA, .)))
mammals <- mammals %>%
mutate(across(c(mass,
wean_mass,
gestation,
max_life,
newborn,
weaning,
litter_size,
afr,
litters_year),
~ifelse(.==-999, NA, .)))
mammals %>%
mutate(mass_kg= mass/1000) %>%
select(genus, species, mass, mass_kg) %>%
arrange(desc(mass_kg))
mammals %>%
ggplot(mapping=aes(x=gestation, y=newborn))+
geom_point()+
geom_smooth(method="lm")
mammals %>%
ggplot(mapping=aes(x=gestation, y=log10(newborn))+
geom_point()+
geom_smooth(method="lm")
mammals %>%
ggplot(mapping=aes(x=gestation, y=log10(newborn)))+
geom_point()+
geom_smooth(method="lm")
mammals %>%
select(genus, species, max_life) %>%
mutate(max_life_new=max_life/12) %>%
arrange(desc(max_life_new))
mammals %>%
ggplot(mapping=aes(x=log10(mass), y=max_life))+
geom_point()+
geom_smooth(method="lm")
